switch (FEAT_ROADSTOPS, SELF, switch_ULGA_07_roadstop, [
	switch_groundsprite_terrain(),
        ]) {
    switch_ULGA_07_roadstop_view;
}

switch (FEAT_ROADSTOPS, SELF, switch_anim_control_value_fruit, [
    STORE_TEMP(
      ( cargo_amount_waiting(FRUT)) > (cargo_check_5) ? 5 :
	  ((cargo_amount_waiting(FRUT)) > (cargo_check_4) ? 4 :
	  ((cargo_amount_waiting(FRUT)) > (cargo_check_3) ? 3 :
	  ((cargo_amount_waiting(FRUT)) > (cargo_check_2) ? 2 :
	  ((cargo_amount_waiting(FRUT)) > (cargo_check_1) ? 1 : 0 ))))
	  , 1),
	  LOAD_TEMP(1) == animation_frame ? CB_RESULT_DO_NOTHING : LOAD_TEMP(1),
    ]) {
	return;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_07_anim_control, ((1 << getbits(extra_callback_info2, 0, 8)) & bitmask(STAT_ANIM_250_TICKS, STAT_ANIM_IS_BUILT)) || getbits(extra_callback_info2, 8, 8)) {
	1: switch_anim_control_value_fruit;
	return CB_RESULT_DO_NOTHING;
}
