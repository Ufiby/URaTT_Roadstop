	RST_VIEW_BAY_SW:          spritelayout_ULGA_03_roadstop_purchase_SW;
	RST_VIEW_BAY_NW:          spritelayout_ULGA_03_roadstop_purchase_NW;
	RST_VIEW_DRIVE_THROUGH_X: spritelayout_ULGA_03_roadstop_purchase_X;
	RST_VIEW_DRIVE_THROUGH_Y: spritelayout_ULGA_03_roadstop_purchase_Y;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_03_roadstop_purchase, [
	switch_groundsprite_terrain(),
        ]) {
    switch_ULGA_03_roadstop_purchase_view;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_03_roadstop_view, view) {
	RST_VIEW_BAY_NE:          spritelayout_ULGA_03_roadstop_NE;
	RST_VIEW_BAY_SE:          spritelayout_ULGA_03_roadstop_SE;
	RST_VIEW_BAY_SW:          spritelayout_ULGA_03_roadstop_SW;
	RST_VIEW_BAY_NW:          spritelayout_ULGA_03_roadstop_NW;
	RST_VIEW_DRIVE_THROUGH_X: spritelayout_ULGA_03_roadstop_X;
	RST_VIEW_DRIVE_THROUGH_Y: spritelayout_ULGA_03_roadstop_Y;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_03_roadstop, [
	switch_groundsprite_terrain(),
        ]) {
    switch_ULGA_03_roadstop_view;
}

switch (FEAT_ROADSTOPS, SELF, switch_anim_control_value_wood, [
    STORE_TEMP(
      ( cargo_amount_waiting(WOOD)) > (cargo_check_5) ? 5 :
	  ((cargo_amount_waiting(WOOD)) > (cargo_check_4) ? 4 :
	  ((cargo_amount_waiting(WOOD)) > (cargo_check_3) ? 3 :
	  ((cargo_amount_waiting(WOOD)) > (cargo_check_2) ? 2 :
	  ((cargo_amount_waiting(WOOD)) > (cargo_check_1) ? 1 : 0 ))))
	  , 1),
	  LOAD_TEMP(1) == animation_frame ? CB_RESULT_DO_NOTHING : LOAD_TEMP(1),
    ]) {
	return;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_03_anim_control, ((1 << getbits(extra_callback_info2, 0, 8)) & bitmask(STAT_ANIM_250_TICKS, STAT_ANIM_IS_BUILT)) || getbits(extra_callback_info2, 8, 8)) {
	1: switch_anim_control_value_wood;
	return CB_RESULT_DO_NOTHING;
}
//===========================================================
item (FEAT_ROADSTOPS, item_ULGA_03)  {
    property {
        class: 					"ULGA";
        availability_type:		RST_AVAILABILITY_TYPE_FREIGHT;
		name: 					string(STR_NAME_WOOD);
		animation_info:			[ANIMATION_NON_LOOPING, 1];
		animation_triggers:     bitmask(ANIM_TRIGGER_ROAD_STOP_NEW_CARGO, ANIM_TRIGGER_ROAD_STOP_CARGO_TAKEN, ANIM_TRIGGER_ROAD_STOP_250_TICKS, ANIM_TRIGGER_ROAD_STOP_BUILT);
    }
    graphics {
        default: 				switch_ULGA_03_roadstop;
		purchase:				switch_ULGA_03_roadstop_purchase;
		anim_control:			switch_ULGA_03_anim_control;
    }
}
