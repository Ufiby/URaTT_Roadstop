    2..255: switch_groundsprite_terrain_snow_4;
    1: switch_groundsprite_terrain_snow_3;
    0: switch_groundsprite_terrain_snow_2;
   -1: switch_groundsprite_terrain_snow_1;
    switch_groundsprite_terrain_normal;
}

