switch (FEAT_ROADSTOPS, SELF, switch_anim_control_value_mineral, [
    STORE_TEMP(
      ( cargo_amount_waiting(COAL)) > (cargo_check_5) ? 5 :
	  ((cargo_amount_waiting(COAL)) > (cargo_check_4) ? 4 :
	  ((cargo_amount_waiting(COAL)) > (cargo_check_3) ? 3 :
	  ((cargo_amount_waiting(COAL)) > (cargo_check_2) ? 2 :
	  ((cargo_amount_waiting(COAL)) > (cargo_check_1) ? 1 : 0 ))))
	  , 1),
	  LOAD_TEMP(1) == animation_frame ? CB_RESULT_DO_NOTHING : LOAD_TEMP(1),
    STORE_TEMP(
      ( cargo_amount_waiting(IORE)) > (cargo_check_5) ? 10 :
	  ((cargo_amount_waiting(IORE)) > (cargo_check_4) ?  9 :
	  ((cargo_amount_waiting(IORE)) > (cargo_check_3) ?  8 :
	  ((cargo_amount_waiting(IORE)) > (cargo_check_2) ?  7 :
	  ((cargo_amount_waiting(IORE)) > (cargo_check_1) ?  6 : 0 ))))
	  , 2),
	  LOAD_TEMP(2) == animation_frame ? CB_RESULT_DO_NOTHING : LOAD_TEMP(2),
    STORE_TEMP(
      ( cargo_amount_waiting(CORE)) > (cargo_check_5) ? 15 :
	  ((cargo_amount_waiting(CORE)) > (cargo_check_4) ? 14 :
	  ((cargo_amount_waiting(CORE)) > (cargo_check_3) ? 13 :
	  ((cargo_amount_waiting(CORE)) > (cargo_check_2) ? 12 :
	  ((cargo_amount_waiting(CORE)) > (cargo_check_1) ? 11 : 0 ))))
	  , 3),
	  LOAD_TEMP(3) == animation_frame ? CB_RESULT_DO_NOTHING : LOAD_TEMP(3),
	  LOAD_TEMP(1) + LOAD_TEMP(2) + LOAD_TEMP(3)
    ]) {
	return;
}

switch (FEAT_ROADSTOPS, SELF, switch_ULGA_04_anim_control, ((1 << getbits(extra_callback_info2, 0, 8)) & bitmask(STAT_ANIM_250_TICKS, STAT_ANIM_IS_BUILT)) || getbits(extra_callback_info2, 8, 8)) {
	1: switch_anim_control_value_mineral;
	return CB_RESULT_DO_NOTHING;
}
